<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sign Up</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="/styles/signUp.css">
</head>
<body class="align">
    <%- include("partials/header.ejs") %>
    <section class="align-items">
        <form class="row g-3">
            <div class="col-md-4">
              <label for="validationDefault01" class="form-label">First name</label>
              <input type="text" class="form-control" id="validationDefault01" value="Enter Your First Name" required>
            </div>
            <div class="col-md-4">
              <label for="validationDefault02" class="form-label">Last name</label>
              <input type="text" class="form-control" id="validationDefault02" value="Enter Your Last Name" required>
            </div>
            <div class="col-md-4">
              <label for="validationDefaultUsername" class="form-label">Email</label>
              <div class="input-group">
                <span class="input-group-text" id="inputGroupPrepend2">@</span>
                <input type="text" class="form-control" id="validationDefaultUsername"  aria-describedby="inputGroupPrepend2" required value="Enter Your Email">
              </div>
            </div>
            <div class="col-md-4">
                <label for="inputPassword" class="col-sm-2 col-form-label">Password</label>
                  <input type="password" class="form-control" id="inputPassword" value="Enter Your Password">
              </div>
            <div class="col-md-4">
              <label for="validationDefault03" class="form-label">City</label>
              <input type="text" class="form-control" id="validationDefault03" required value="Enter Your City">
            </div>
            <div class="col-md-4">
              <label for="validationDefault04" class="form-label">State</label>
              <select class="form-select" id="validationDefault04" required>
                <option selected disabled value="">Choose...</option>
                <option>Maharashtra</option>
                <option>karnataka</option>
              </select>
            </div>
            <div class="sub-button">
              <button class="btn btn-primary" type="submit">Submit form</button>
            </div>
          </form>
    </section>
</body>
</html>